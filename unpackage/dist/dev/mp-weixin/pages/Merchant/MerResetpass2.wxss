
.content {
	height: 1200rpx;
}
.uni-password-but {
	margin: 0rpx auto;
	width: 700rpx;
	padding: 53rpx 0;
}
.uni-password-but input {
	width: 655rpx;
	height: 80rpx;
	background-color: #ffffff;
	border-radius: 40px;
	border: solid 1px #dbdbdc;
	padding-left: 45rpx;
	font-size: 32rpx;
	margin-bottom: 25rpx;
}
.uni-password-but button {
	margin-top: 75rpx;
	border-radius: 40rpx;
	background-color: #ffca30;
	font-size: 32rpx;
	color: #3C3C3C;
	letter-spacing: 2rpx;
}

