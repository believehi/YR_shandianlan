
.un-video-box{padding-top: 27rpx;}
.un-video-list{display: -webkit-box;display: -webkit-flex;display: flex;-webkit-box-pack: justify;-webkit-justify-content: space-between;justify-content: space-between;-webkit-flex-wrap: wrap ;flex-wrap: wrap ;padding: 0 15rpx;}
.un-video-item{margin-bottom: 40rpx;}
.un-video-item image{width: 329rpx;height: 183rpx;border-radius: 15rpx;overflow: hidden;}
.un-video-text text{width: 329rpx;overflow: hidden;display: inline-block;}
.un-video-item view text{font-size: 28rpx;color: #3e3e3e;}
.un-video-item view {line-height: 32rpx;}

