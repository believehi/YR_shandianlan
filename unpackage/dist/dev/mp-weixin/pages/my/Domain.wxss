
.content {
	height: 1200rpx;
}
.uni-item {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-flex-wrap: wrap-reverse;
	        flex-wrap: wrap-reverse;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
}
.uni-domain-list {
	width: 635rpx;
	height: 90rpx;
	background: #FFFFFF;
	border-radius: 20rpx;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	padding: 0 25rpx 0 40rpx;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	margin-bottom: 15rpx;
	box-shadow: 0px 0px 25px 0px rgba(0, 0, 0, 0.06);
}
.uni-domain-list:last-child {
	margin-top: 15rpx;
}
.uni-domain-list text {
	font-size: 32rpx;
	color: #1a1a1a;
}
.uni-domain-list .uni-pt {
	color: #909090;
	-webkit-box-flex: 2;
	-webkit-flex-grow: 2;
	        flex-grow: 2;
	text-align: right;
}
.uni-item-list {

	width: 700rpx;
	height: 90rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	border-radius: 20rpx;
	font-size: 34rpx;
	color: #FFFFFF;
}
.uni-item-list text {
	margin-left: 10rpx;
	line-height: 90rpx;
}
.uni-tj-but {
	margin-top: 15rpx;
	background-color: #fbb42b;
}

