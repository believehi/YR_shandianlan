@import "../../common/vendor.wxss";

.content {
  padding-top: 15rpx;
  height: 1200rpx;
}
.content view {
  padding: 25rpx 25rpx 25rpx 30rpx;
  background: #FFFFFF;
  margin-bottom: 15rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
  justify-content: space-between;
}
.content view text {
  font-size: 32rpx;
}