@import "../../common/vendor.wxss";

.content {
  height: 1200rpx;
  padding-top: 20rpx;
}
.uni-jl-list {
  background-color: #FFFFFF;
}
.content .uni-jl-list:first-child {
  margin-bottom: 20rpx;
}
.uni-jl-list .uni-jl-item {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
  justify-content: space-between;
  line-height: 90rpx;
  padding: 0 25rpx;
  font-size: 30rpx;
  background-color: #FFFFFF;
}
.uni-jl-list1 {
  background: #FFFFFF;
}
.uni-jl-list1 .uni-jl-item {
  border-bottom: 1rpx solid #e6e6e6;
  width: 700rpx;
  margin: 0 auto;
  padding: 0rpx !important;
}
.uni-jl-list1 .uni-jl-item:last-child {
  border-bottom: none;
}
.uni-jl-item .uni-jl {
  -webkit-box-flex: 2;
  -webkit-flex-grow: 2;
  flex-grow: 2;
}
.uni-jl-item .uni-jl-sum {
  font-size: 28rpx;
  color: #FBB42B;
}
.uni-jl-item ._button {
  width: 138rpx;
  height: 55rpx;
  background-color: #FBB42B;
  border-radius: 28rpx;
  font-size: 30rpx;
  color: #FFFFFF;
  line-height: 55rpx;
}