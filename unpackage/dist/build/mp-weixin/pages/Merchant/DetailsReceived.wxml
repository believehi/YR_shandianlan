<view class="content"><uni-segmented-control vue-id="1" current="{{current}}" values="{{items}}" style-type="button" active-color="#4cd964" data-event-opts="{{[['^clickItem',[['onClickItem']]]]}}" bind:clickItem="__e" bind:__l="__l"></uni-segmented-control><view class="content"><view hidden="{{!(current===0)}}"><block wx:for="{{rwlistArray}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="uni-ts-box"><view class="uni-ts-item"><image src="{{item.Headimgurl}}"></image><view class="uni-text-box"><view class="uni-top-text"><text class="uni-ts-title Text_Nowrap">{{item.NickName}}</text><text class="uni-ts-title Text_Nowrap1">{{"领取时间"+item.Receivedate}}</text></view><view class="uni-item-box"><view class="uni-time-item"><uni-icon class="uni-time-icon" vue-id="{{'2-'+index}}" bind:__l="__l"></uni-icon><text>{{item.Createdate}}</text></view><view class="uni-time-item"><uni-icon class="uni-phones-icon" vue-id="{{'3-'+index}}" bind:__l="__l"></uni-icon><text>{{item.phone==null?'未绑定手机号码':item.phone}}</text></view></view></view></view></view></block></view><view hidden="{{!(current===1)}}"><block wx:for="{{rwlistArray}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="uni-ts-box"><view class="uni-ts-item"><image src="{{item.Headimgurl}}"></image><view class="uni-text-box"><view class="uni-top-text"><text class="uni-ts-title Text_Nowrap">{{item.NickName}}</text><text class="uni-ts-title Text_Nowrap1">{{"到店时间"+item.Receivedate}}</text></view><view class="uni-item-box"><view class="uni-time-item"><uni-icon class="uni-time-icon" vue-id="{{'4-'+index}}" bind:__l="__l"></uni-icon><text>{{item.Createdate}}</text></view><view class="uni-time-item"><uni-icon class="uni-phones-icon" vue-id="{{'5-'+index}}" bind:__l="__l"></uni-icon><text>{{item.phone==null?'未绑定手机号码':item.phone}}</text></view></view></view></view></view></block></view></view><text class="{{[(Tishi=='暂无任务')?'tisi':'']}}">{{Tishi}}</text><text class="uni-ti-shi">{{bottomtisi}}</text><view class="nui-nav-bot"><view data-event-opts="{{[['tap',[['openindex',['$event']]]]]}}" class="nui-nav-item" bindtap="__e"><image src="../../static/Home.png" mode></image><text>首页</text></view><view class="nui-nav-item"><image src="../../static/tesks.png" mode></image><text>任务</text></view><view data-event-opts="{{[['tap',[['openInfo',['$event']]]]]}}" class="nui-nav-item" bindtap="__e"><image src="../../static/my.png" mode></image><text>我的</text></view></view></view>