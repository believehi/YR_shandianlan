
.content {
	height: 1200rpx;
}
.uni-sctp-icon {
	margin-top: 65rpx;
}
.uni-cz-box {
	padding: 0 4%;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	    -ms-flex-pack: justify;
	        justify-content: space-between;
}
.uni-cz-item:nth-child(2),
.uni-cz-item:nth-child(3) {
	margin-left: 30rpx;
}
.uni-cz-item image {
	width: 175rpx;
	height: 320rpx;
	margin-top: 10rpx;
}
.uni-cz-item text {
	font-size: 32rpx;
	color: #1a1a1a;
}
.uni-item {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	    -ms-flex-pack: center;
	        justify-content: center;
}
.uni-domain-text {
	width: 270rpx;
}
.uni-domain-list {
	width: 635rpx;
	height: 90rpx;
	background: #FFFFFF;
	border-radius: 20rpx;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	padding: 0 25rpx 0 40rpx;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	    -ms-flex-pack: justify;
	        justify-content: space-between;
	margin-bottom: 15rpx;
	-webkit-box-shadow: 0px 0px 25px 0px rgba(0, 0, 0, 0.06);
	        box-shadow: 0px 0px 25px 0px rgba(0, 0, 0, 0.06);
}
.uni-domain-list:last-child {
	margin-top: 15rpx;
}
.uni-domain-list text,
.uni-input {
	font-size: 32rpx;
	color: #1a1a1a;
}
.uni-input {
	text-align: right;
}
.uni-domain-list .uni-pt {
	color: #1a1a1a;
	text-align: right;
	font-size: 32rpx;
}
.uni-item-list {

	width: 700rpx;
	height: 90rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	    -ms-flex-pack: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	border-radius: 20rpx;
	font-size: 34rpx;
	color: #FFFFFF;
}
.uni-item-list text {
	margin-left: 10rpx;
	line-height: 90rpx;
}
.uni-tj-but {
	margin-top: 15rpx;
	background-color: #fbb42b;
}

